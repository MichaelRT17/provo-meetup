<article class="admin-page">
  <section class="title-bar">
    <h1 class="title">Sms Admin Page</h1>
  </section>
  <section class="side-bar">
    <a class="registered-numbers" href="registerednumbers">
      Registered Numbers
    </a>
    <a class="subscription-requests" href="subscriptionrequests">
      Subscription Requests
    </a>
    <a class="stop-numbers" href="stopnumbers">
      Stop Numbers
    </a>
    <a class="admin-numbers" href="adminnumbers">
      Admin Numbers
    </a>
    <a class="twilio-numbers" href="twilionumbers">
      Twilio Numbers
    </a>
  </section>
  <section class="number-list-section">
    <ul class="number-list">
      <li><%= @conn.assigns.selected_list %></li>
      <li class="sms-row">
        <input class="checkbox" type="checkbox" />
        <span class="number">123-456-7890</span>
      </li>
      <li class="new-number">
        <input class="number" type="input" placeholder="555-123-4567"/>
        <button class="addbtn">Add</button>
      </li>
    </ul>
  </section>
</article>
